{
  "name": "rain-protocol-subgraph",
  "description": "Rain protocol subgraph",
  "version": "0.2.0",
  "author": "Beehive Innovation",
  "license": "CAL",
  "repository": {
    "type": "git",
    "url": "https://github.com/beehive-innovation/rain-protocol-subgraph.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/beehive-innovation/rain-protocol-subgraph/issues"
  },
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "generate-schema": "cat schema/* > schema.graphql",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ vishalkale151071/rain-protocol",
    "deploy-rain": "graph deploy --node https://api.thegraph.com/deploy/ beehive-innovation/rain-protocol-v2-mumbai",
    "deploy-rain-polygon": "graph deploy --node https://api.thegraph.com/deploy/ beehive-innovation/rain-protocol-v2-polygon",
    "deploy:blocks": "graph deploy --node https://api.thegraph.com/deploy/ vishalkale151071/blocks",
    "create-local": "graph create --node http://localhost:8020/ vishalkale151071/rain-protocol",
    "remove-local": "graph remove --node http://localhost:8020/ vishalkale151071/rain-protocol",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 vishalkale151071/rain-protocol  --version-label 1",
    "prepare:mumbai": "yarn mustache config/mumbai.json subgraph.template.yaml subgraph.yaml",
    "deploy-build:mumbai": "yarn prepare:mumbai && yarn generate-schema && yarn codegen && yarn build && yarn deploy",
    "prepare:matic": "yarn mustache config/polygon.json subgraph.template.yaml subgraph.yaml",
    "deploy-build:matic": "yarn prepare:matic && yarn generate-schema && yarn codegen && yarn build && yarn deploy",
    "prepare:localhost": "npx mustache config/localhost.json subgraph.template.yaml subgraph.yaml",
    "deploy-build:localhost": "npm run prepare:localhost && npm run generate-schema && npm run codegen && npm run build && npm run create-local && npm run deploy-local",
    "lint-test": "eslint './test/**/*.ts'",
    "hardhat-node": "hardhat node",
    "graph-node": "docker-compose -f \"docker/docker-compose.yml\" up",
    "test": "hardhat test"
  },
  "devDependencies": {
    "@beehiveinnovation/balancer-core": "^0.0.7",
    "@beehiveinnovation/configurable-rights-pool": "^0.0.1",
    "@beehiveinnovation/rain-protocol": "git+https://github.com/beehive-innovation/rain-protocol.git#7f81d6c808bc79c2a45dc2fe667133a870561616",
    "@beehiveinnovation/rain-statusfi": "git+https://github.com/beehive-innovation/rain-statusfi.git#288e082ae81ca796b0732619a9631db24beb7a7a",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^3.1.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@vishalkale15107/rain-protocol": "git+https://github.com/vishalkale151071/rain-protocol.git#3d0ee716430d28716365f0f1c999fe14c30f0b4b",
    "apollo-fetch": "^0.7.0",
    "chai": "^4.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.8.3",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "ts-node": "^10.1.0",
    "typechain": "^6.1.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.21.0",
    "@graphprotocol/graph-ts": "^0.20.0",
    "mustache": "^3.2.1"
  }
}