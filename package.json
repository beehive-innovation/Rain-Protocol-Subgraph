{
  "name": "@beehiveinnovation/rain-protocol-subgraph",
  "description": "Rain protocol subgraph",
  "version": "0.2.0",
  "author": "Beehive Innovation",
  "license": "CAL",
  "repository": {
    "type": "git",
    "url": "https://github.com/beehive-innovation/rain-protocol-subgraph.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/beehive-innovation/rain-protocol-subgraph/issues"
  },
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "generate-schema": "cat schema/* > schema.graphql",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ vishalkale151071/rain-protocol",
    "deploy-rain": "graph deploy --node https://api.thegraph.com/deploy/ beehive-innovation/rain-protocol-v2-mumbai",
    "deploy-rain_v3": "graph deploy --node https://api.thegraph.com/deploy/ beehive-innovation/rain-protocol-mumbai-v3",
    "prepare:mumbai_v3": "npx mustache config/mumbai/mumbai_5a35b0.json subgraph.template.yaml subgraph.yaml",
    "deploy-rain-protocol-v2_1-mumbai": "graph deploy --node https://api.thegraph.com/deploy/ beehive-innovation/rain-protocol-v2_1-mumbai",
    "deploy-rain-polygon": "graph deploy --node https://api.thegraph.com/deploy/ beehive-innovation/rain-protocol-v2-polygon",
    "deploy:blocks": "graph deploy --node https://api.thegraph.com/deploy/ vishalkale151071/blocks",
    "create-local": "graph create --node http://localhost:8020/ beehive-innovation/rain-protocol",
    "remove-local": "graph remove --node http://localhost:8020/ beehive-innovation/rain-protocol",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 beehive-innovation/rain-protocol  --version-label 1",
    "prepare:mumbai": "npx mustache config/mumbai/mumbai_1.json subgraph.template.yaml subgraph.yaml",
    "deploy-build:mumbai": "npm run prepare:mumbai && npm run generate-schema && npm run codegen && npm run build && npm run deploy",
    "prepare:matic": "npx mustache config/polygon/polygon.json subgraph.template.yaml subgraph.yaml",
    "deploy-build:matic": "npm run prepare:matic && npm run generate-schema && npm run codegen && npm run build && npm run deploy-rain-polygon",
    "prepare:localhost": "npx mustache config/localhost.json subgraph.template.yaml subgraph.yaml",
    "deploy-build:localhost": "npm run prepare:localhost && npm run generate-schema && npm run codegen && npm run build && npm run create-local && npm run deploy-local",
    "hardhat-node": "hardhat node",
    "graph-node": "docker-compose -f \"docker/docker-compose.yml\" up",
    "m1-graph-node": "docker-compose -f \"graph-node/docker/docker-compose.yml\" up",
    "test": "hardhat test",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "prettier": "prettier . '!./docker' --check",
    "prettier-write": "prettier . '!./docker' --write",
    "deploy-subgraph": "ts-node scripts/index.ts"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.35.0",
    "@graphprotocol/graph-ts": "^0.28.1",
    "0xsequence": "^0.39.0",
    "mustache": "^3.2.1"
  },
  "devDependencies": {
    "@beehiveinnovation/rain-protocol": "git+https://github.com/beehive-innovation/rain-protocol.git#d35c301661393f8aa70bc808d1f8fbeefe3c2a6a",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^3.1.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.21",
    "commander": "^9.4.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "apollo-fetch": "^0.7.0",
    "chai": "^4.2.0",
    "dotenv": "^10.0.0",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-contract-sizer": "^2.1.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",   
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.9.9",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "ts-node": "^10.1.0",
    "typechain": "^6.1.0",
    "typescript": "^4.5.2"
  }
}
