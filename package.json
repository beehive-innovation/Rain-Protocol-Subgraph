{
  "name": "rain-protocol",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "prepare:mumbai": "yarn mustache config/mumbai.json subgraph.template.yaml subgraph.yaml",
    "deploy-build:mumbai": "yarn prepare:mumbai && yarn codegen && yarn build && yarn deploy:blocks",
    "prepare:matic": "yarn mustache config/polygon.json subgraph.template.yaml subgraph.yaml",
    "deploy-build:matic": "yarn prepare:matic && yarn codegen && yarn build && yarn deploy",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ vishalkale151071/rain-protocol",
    "deploy:blocks": "graph deploy --node https://api.thegraph.com/deploy/ vishalkale151071/blocks",
    "create-local": "graph create --node http://localhost:8020/ vishalkale151071/rain-protocol",
    "remove-local": "graph remove --node http://localhost:8020/ vishalkale151071/rain-protocol",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 vishalkale151071/rain-protocol",
    "test": "yarn hardhat test",
    "lint": "eslint */**.ts",
    "prepare:localhost": "yarn mustache config/localhost.json subgraph.template.yaml subgraph.yaml",
    "deploy-build:localhost": "yarn prepare:localhost && yarn codegen && yarn build && yarn create-local && yarn deploy-local",
    "hh-node": "yarn hardhat node",
    "graph-node": "docker-compose -f \"docker/docker-compose.yml\" up",
    "deploy-test": "yarn hardhat run scripts/deploy-test-factories.ts"
  },
  "devDependencies": {
    "@beehiveinnovation/balancer-core": "^0.0.7",
    "@beehiveinnovation/configurable-rights-pool": "^0.0.1",
    "@graphprotocol/graph-cli": "0.21.0",
    "@graphprotocol/graph-ts": "0.20.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^2.1.3",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "apollo-fetch": "^0.7.0",
    "chai": "^4.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.8.2",
    "mustache": "^3.2.1",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "typechain": "^5.1.2",
    "typescript": "^4.5.2"
  }
}
