{
  "name": "rain-protocol",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "prepare:mumbai": "yarn mustache config/mumbai.json subgraph.template.yaml subgraph.yaml",
    "deploy-build:mumbai": "yarn prepare:mumbai && yarn codegen && yarn build && yarn deploy",
    "prepare:mainnet": "yarn mustache config/mainnet.json subgraph.template.yaml subgraph.yaml",
    "deploy-build:mainnet": "yarn prepare:mainnet && yarn codegen && yarn build && yarn deploy",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ vishalkale151071/rain-protocol",
    "create-local": "graph create --node http://localhost:8020/ vishalkale151071/rain-protocol",
    "remove-local": "graph remove --node http://localhost:8020/ vishalkale151071/rain-protocol",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 vishalkale151071/rain-protocol"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.21.0",
    "@graphprotocol/graph-ts": "0.20.0",
    "mustache": "^3.2.1"
  }
}
